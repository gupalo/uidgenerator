{
    "name": "gupalo/uidgenerator",
    "description": "UID Generator",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\UidGenerator\\": "src/UidGenerator/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\Tests\\UidGenerator\\": "tests/UidGenerator/"
        }
    }
}
